<?xml version="1.0"?>
<launch>

  <include file="$(find neu_launch)/launch/laser.launch" />
  <include file="$(find neu_launch)/launch/rosserial.launch" />
  <include file="$(find neu_launch)/launch/rosserial2.launch" />
  <include file="$(find neu_launch)/launch/amcl_diff.launch" />
  <include file="$(find neu_launch)/launch/move_base-hj.launch" />
  <include file="$(find neu_launch)/launch/arduino.launch" />
  <!-- Run the map server with a blank map + obstacle -->
  <node name="odom_tf_package" pkg="odom_tf_package" type="odom_tf_package" />
  <node name="map_server" pkg="map_server" type="map_server" args="$(find neu_launch)/maps/map.yaml"/>

  <node pkg="rviz" type="rviz" name="rviz" 
    args="-d $(find neu_launch)/config/hj_slam.rviz"/>
<!--node name="move_base_square" pkg="move_base_square" type="move_base_square" /-->

</launch>
